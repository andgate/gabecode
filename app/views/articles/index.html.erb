<% provide(:title, "Articles") %>

<h1>Articles</h1>

<% @articles.each do |article| %>
    <div class="panel panel-default">
        <div class="panel-heading">
            <h2><%= article.title %></h2> posted on <%= article.created_at %>
        </div>
        <div class="panel-body">
            <%= article.text %>
        </div>
    </div>
<% end %>